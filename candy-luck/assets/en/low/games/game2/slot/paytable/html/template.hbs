<div id='paytable' class='paytable-wrapper'>
  <div class='paytable-overlay'></div>

  <div class='paytable'>
    <div class='paytable-header'>
      <div class='paytable-actions left'>
        <div class='paytable-icon' id='close-paytable' title='{{content.close}}'>
          <svg
            viewBox='0 0 24 24'
            width='24'
            height='24'
            fill='none'
            stroke='#a02c7e'
            stroke-width='5.0'
            stroke-linecap='round'
            stroke-linejoin='round'
            xmlns='http://www.w3.org/2000/svg'
          >
            <path d='M18 4L10 12L18 20' />
          </svg>
        </div>
      </div>

      <div class='paytable-title'>
        Paytable
      </div>

      <div class='paytable-actions right'>
        <div class='paytable-icon circle' id='volume-mute' title='{{content.mute}}'>
          <svg viewBox='0 0 24 24' xmlns='http://www.w3.org/2000/svg'><g
              fill-rule='evenodd'
              clip-rule='evenodd'
              clip-path='url(#a)'
